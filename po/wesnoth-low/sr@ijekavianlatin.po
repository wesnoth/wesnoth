msgstr "Šar"

#. [set_variable]
#: data/campaigns/Legend_of_Wesmere/scenarios/chapter3/09_Bounty_Hunters.cfg:212
msgid "some experienced warriors"
msgstr "nešto iskusnih ratnika"

