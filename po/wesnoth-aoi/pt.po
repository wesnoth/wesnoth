msgid "Trolls"
msgstr "Trogloditas"
#
# File: data/campaigns/An_Orcish_Incursion/scenarios/04_Valley_of_Trolls.cfg, line: 81
# File: data/campaigns/An_Orcish_Incursion/scenarios/04_Valley_of_Trolls.cfg, line: 81
# File: data/campaigns/An_Orcish_Incursion/scenarios/04_Valley_of_Trolls.cfg, line: 81
