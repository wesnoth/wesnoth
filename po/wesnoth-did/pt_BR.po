#~ msgstr "Foragidos"

#~ msgid "Dap Horner"
#~ msgstr "Dap Horner"

#~ msgid "Watchman"
#~ msgstr "Vigia"

#~ msgid "Gorak Cole"
#~ msgstr "Gorak Cole"

