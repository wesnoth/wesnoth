#~ msgstr "короткий меч"

#~ msgid "chill wave"
#~ msgstr "волна холода"

#~ msgid "shadow wave"
#~ msgstr "волна теней"

#~ msgid "staff"
#~ msgstr "посох"

#~ msgid "sling"
#~ msgstr "праща"
