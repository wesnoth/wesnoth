#. [objective]: condition=lose
# File: data/campaigns/Descent_Into_Darkness/scenarios/02_Peaceful_Valley.cfg, line: 87
# File: data/campaigns/Descent_Into_Darkness/scenarios/03_A_Haunting_in_Winter.cfg, line: 128
# File: data/campaigns/Descent_Into_Darkness/scenarios/04_Beginning_of_the_Revenge.cfg, line: 122
# File: data/campaigns/Descent_Into_Darkness/scenarios/05_Orc_War.cfg, line: 190
# File: data/campaigns/Descent_Into_Darkness/scenarios/07_A_Small_Favor.cfg, line: 213
# File: data/campaigns/Descent_Into_Darkness/scenarios/08_A_Small_Favor2.cfg, line: 73
# File: data/campaigns/Descent_Into_Darkness/scenarios/08_A_Small_Favor2.cfg, line: 398
# File: data/campaigns/Descent_Into_Darkness/scenarios/09_A_Small_Favor3.cfg, line: 102
