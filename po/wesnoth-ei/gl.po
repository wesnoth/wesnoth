msgstr "Ouen"

#. [side]: type=Revenant, id=Lanar-Skal
#: data/campaigns/Eastern_Invasion/scenarios/05_Northern_Outpost.cfg:61
msgid "Lanar-Skal"
msgstr "Lanar-Escal"

