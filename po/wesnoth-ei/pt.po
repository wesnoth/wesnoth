msgid "Move Gweddry to Weldyn castle"
msgstr "Leva Gweddry até o castelo de Weldyn"
#
# File: data/campaigns/Eastern_Invasion/scenarios/14_Approaching_Weldyn.cfg, line: 147
# File: data/campaigns/Eastern_Invasion/scenarios/16_Weldyn_under_Attack.cfg, line: 179
