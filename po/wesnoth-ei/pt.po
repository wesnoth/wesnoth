msgid "Mal-Marak"
msgstr "Mal-Marak"
#
# File: data/campaigns/Eastern_Invasion/scenarios/04c_Mal-Ravanals_Capital.cfg, line: 69
# File: data/campaigns/Eastern_Invasion/scenarios/04c_Mal-Ravanals_Capital.cfg, line: 69
