# File: data/campaigns/Eastern_Invasion/scenarios/04c_Mal-Ravanals_Capital.cfg, line: 69
msgid "Mal-Galkar"
msgstr "Mal-Galkar"
#
# File: data/campaigns/Eastern_Invasion/scenarios/04c_Mal-Ravanals_Capital.cfg, line: 89
# File: data/campaigns/Eastern_Invasion/scenarios/04c_Mal-Ravanals_Capital.cfg, line: 89
