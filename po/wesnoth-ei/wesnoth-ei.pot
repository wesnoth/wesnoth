msgid "Death of Dacyn"
msgstr ""

#. [objective]: condition=lose
#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:271
msgid "Turns run out"
msgstr ""

#. [message]: speaker=Gweddry
#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:290
msgid "Follow me, men! Through the trapdoor!"
msgstr ""

#. [message]: speaker=Dacyn
#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:304
msgid "Hurry! Before long enemy reinforcements will arrive, and we will all die! Go to the trapdoor!"
msgstr ""

