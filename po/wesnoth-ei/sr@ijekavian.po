#: data/campaigns/Eastern_Invasion/scenarios/09_Castle_in_the_Ice.cfg:921
msgid "Tapok"
msgstr ""

#. [event]
#: data/campaigns/Eastern_Invasion/scenarios/09_Castle_in_the_Ice.cfg:926
msgid "Ogho"
msgstr ""

