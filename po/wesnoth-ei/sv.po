#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:52
#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:219
msgid "Dacyn"
msgstr "Dacyn"

#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:71
msgid "Mal-Sakkat"
msgstr "Mal-Sakkat"

#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:91
msgid "Mal-Talar"
msgstr "Mal-Talar"

#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:108
msgid "Mal-Uldhar"
msgstr "Mal-Uldhar"

#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:131
msgid "Resist until you receive further instructions from Dacyn"
msgstr "Håll ställningarna tills du får fler instruktioner från Dacyn"

#: data/campaigns/Eastern_Invasion/scenarios/01.The_Outpost.cfg:135
