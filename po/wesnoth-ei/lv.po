msgstr ""

#. [side]: type=Sky Drake, id=Kegra
#: data/campaigns/Eastern_Invasion/scenarios/09_Castle_in_the_Ice.cfg:108
msgid "Kegra"
msgstr ""

#. [side]
#: data/campaigns/Eastern_Invasion/scenarios/09_Castle_in_the_Ice.cfg:184
