msgstr ""

#. [event]
#: data/campaigns/Eastern_Invasion/scenarios/09_Castle_in_the_Ice.cfg:255
msgid "Huno"
msgstr ""

#. [event]
#: data/campaigns/Eastern_Invasion/scenarios/09_Castle_in_the_Ice.cfg:258
