msgstr "Tarven"

#. [side]: type=Cavalier, id=Tarwen
#. [side]: type=General, id=Kestrel
#. [side]: type=Master Bowman, id=Quentin
#. [side]
#. [side]: type=Shock Trooper, id=Linneus
#. [side]: type=Lieutenant, id=Archarel
