# type: TH
#: ../../doc/man/wesnoth.6:16 ../../doc/man/wesnothd.6:16
#, no-wrap
msgid "2007"
msgstr "2007"

# type: TH
#: ../../doc/man/wesnoth.6:16
#, no-wrap
msgid "wesnoth"
msgstr "wesnoth"

# type: TH
