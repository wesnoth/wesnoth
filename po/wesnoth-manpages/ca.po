#: doc/man/wesnoth.6:238
#, fuzzy, no-wrap
#| msgid "B<--nocache>"
msgid "B<--nobanner>"
msgstr "B<--nocache>"

#. type: Plain text
#: doc/man/wesnoth.6:241
msgid "suppress the startup banner."
msgstr ""

# type: TP
#. type: TP
#: doc/man/wesnoth.6:241
#, no-wrap
msgid "B<--nomusic>"
