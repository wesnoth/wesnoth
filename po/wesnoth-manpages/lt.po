# type: TP
#. type: TP
#: doc/man/wesnothd.6:224
#, no-wrap
msgid "B<db_user>"
msgstr "B<db_user>"

# type: Plain text
#. type: Plain text
#: doc/man/wesnothd.6:227
#, fuzzy
