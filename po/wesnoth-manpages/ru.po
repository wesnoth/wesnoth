#. type: TP
#: doc/man/wesnothd.6:221
#, no-wrap
msgid "B<db_name>"
msgstr "B<db_name>"

# type: Plain text
#. type: Plain text
#: doc/man/wesnothd.6:224
msgid "The name of the database"
msgstr "Имя базы данных"

