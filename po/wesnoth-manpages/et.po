# type: TP
#. type: TP
#: ../../doc/man/wesnoth.6:233
#, no-wrap
msgid "B<--with-replay>"
msgstr "B<--with-replay>"

# type: Plain text
#. type: Plain text
#: ../../doc/man/wesnoth.6:238
