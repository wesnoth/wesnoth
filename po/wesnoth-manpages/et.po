# type: TP
#. type: TP
#: doc/man/wesnothd.6:91
#, no-wrap
msgid "B<-v, --verbose>"
msgstr "B<-v, --verbose>"

# type: Plain text
#. type: Plain text
#: doc/man/wesnothd.6:94
msgid "turns debug logging on."
