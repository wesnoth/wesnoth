#. type: SH
#: doc/man/wesnothd.6:80
#, no-wrap
msgid "SERVER CONFIG"
msgstr "CONFIGURAÇAO DO SERVIDOR"

# type: SS
#. type: SS
#: doc/man/wesnothd.6:82
#, no-wrap
msgid "The general syntax is:"
msgstr "A sintaxe geral é:"

# type: Plain text
#. type: Plain text
#: doc/man/wesnothd.6:86
msgid "[I<tag>]"
msgstr "[I<tag>]"

# type: Plain text
#. type: Plain text
#: doc/man/wesnothd.6:88
msgid "I<key>=\"I<value>\""
