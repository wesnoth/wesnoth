#. type: TP
#: doc/man/wesnoth.6:381
#, no-wrap
msgid "B<--validcache>"
msgstr "B<--validcache>"

#. type: Plain text
#: doc/man/wesnoth.6:384
msgid "assumes that the cache is valid. (dangerous)"
msgstr "asume que el caché es válido (peligroso)."

#. type: TP
#: doc/man/wesnoth.6:384
#, no-wrap
msgid "B<-v, --version>"
msgstr "B<-v, --version>"

#. type: Plain text
#: doc/man/wesnoth.6:387
