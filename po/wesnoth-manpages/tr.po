# type: TH
#. type: Plain text
#: doc/man/wesnothd.6:293
msgid "B<wesnoth>(6)"
msgstr "B<wesnoth>(6)"

# type: TP
#, no-wrap
#~ msgid "B<--script>I<\\ file>"
#~ msgstr "B<--script>I<\\ dosya>"

#~ msgid ""
#~ "(experimental)  I<file> containing a Lua script to control the client."
#~ msgstr "istemciyi yönetecek Lua betiğini içeren I<dosya> (deneyseldir)."

# type: TP
#, no-wrap
#~ msgid "B<--nodelay>"
#~ msgstr "B<--nodelay>"

# type: Plain text
#~ msgid ""
#~ "runs the game without any delays for graphic benchmarking. This is "
#~ "automatically enabled by B<--nogui>."
#~ msgstr ""
#~ "çizgesel başarımı ölçümlemek amacıyla oyunu gecikmeler olmaksızın "
#~ "çalıştırır. Bu seçenek B<--nogui> seçeneği tarafından kendiliğinden etkin "
#~ "kılınır."

# type: TP
#, no-wrap
#~ msgid "B<--bunzip2>I<\\ infile.bz2>"
#~ msgstr "B<--bunzip2>I<\\ dosya.bz2>"

# type: Plain text
#~ msgid ""
#~ "decompresses a file which should be in bzip2 format and stores it without "
