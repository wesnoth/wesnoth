#: data/campaigns/Heir_To_The_Throne/scenarios/16_Hasty_Alliance.cfg:93
msgid "Li’sar"
msgstr "Li’sar"

#. [side]: type=Princess, id=Li'sar
#. [side]: type=General, id=Robryn
#. [side]: type=General, id=Josephus
