#: data/campaigns/Heir_To_The_Throne/scenarios/15_The_Lost_General.cfg:4
msgid "The Lost General"
msgstr "O General Perdido"

#
# File: data/campaigns/Heir_To_The_Throne/scenarios/15_The_Lost_General.cfg, line: 64
# File: data/campaigns/Heir_To_The_Throne/scenarios/15_The_Lost_General.cfg, line: 64
