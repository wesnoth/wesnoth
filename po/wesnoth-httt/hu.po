msgid "Mriram"
msgstr "Mriram"

#: data/campaigns/Heir_To_The_Throne/scenarios/Bay_of_Pearls.cfg:353
#, fuzzy
msgid "Free at last! Now death to the orcs!"
msgstr "Végre szabadok vagyunk! Halál az orkokra!"

#: data/campaigns/Heir_To_The_Throne/scenarios/Bay_of_Pearls.cfg:374
msgid "Mabooa"
msgstr "Mabooa"

#: data/campaigns/Heir_To_The_Throne/scenarios/Bay_of_Pearls.cfg:389
