#. [then]
#: data/campaigns/Heir_To_The_Throne/scenarios/23_Test_of_the_Clans.cfg:124
#: data/campaigns/Heir_To_The_Throne/scenarios/24_Battle_for_Wesnoth.cfg:203
msgid "Sir Ruga"
msgstr "Sir Ruga"

#
# File: data/campaigns/Heir_To_The_Throne/scenarios/23_Test_of_the_Clans.cfg, line: 153
# File: data/campaigns/Heir_To_The_Throne/scenarios/24_Battle_for_Wesnoth.cfg, line: 194
