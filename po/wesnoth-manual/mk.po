#. type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: doc/manual/manual.en.xml:2185
msgid "kill bonus"
msgstr ""

# type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#. type: Content of: <book><chapter><section><section><table><tgroup><thead><row><entry>
#: doc/manual/manual.en.xml:2188
msgid "fighting bonus"
msgstr ""

