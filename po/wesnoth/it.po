#. [multiplayer_side]: id=Custom
#: data/_main.cfg:22
msgid "Custom"
msgstr "Personalizzata"

#. [advanced_preference]: type=combo
#: data/advanced_preferences.cfg:4
msgid "Compress saved games"
msgstr "Comprimi le partite salvate"

#. [option]: id=gzip
#: data/advanced_preferences.cfg:9
