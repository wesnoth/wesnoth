"\n"
"\t\tprefix=$lake{!}|$base{!}\n"
"\t\tsuffix= de la Bahía| del Puerto\n"
"\t\tmain={prefix}{suffix}\n"
"\t"

# Accidentes geográficos en corrientes de agua
#. [village_naming]
#: data/english.cfg:76
