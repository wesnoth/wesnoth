"\n"
"\t\tsuffix=ham|ton|field\n"
"\t\tmain=$base{!}{suffix}\n"
"\t"
msgstr ""
"\n"
"\t\tsuffix= del Lecho| del Paso| del Campo\n"
"\t\tmain=$base{!}{suffix}\n"
"\t"

# Tipos variados de bosques
