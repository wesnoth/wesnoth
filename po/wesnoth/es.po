"\t\tprefix=$base{!}|$river{!}|$bridge{!}\n"
"\t\tsuffix=bridge|bridge|ham|ton\n"
"\t\tmain={prefix}{suffix}\n"
"\t"
msgstr ""
"\n"
"\t\tprefix=$base{!}|$river{!}|$bridge{!}\n"
"\t\tsuffix= del Puente| del Vado| del Lecho| del Paso\n"
"\t\tmain={prefix}{suffix}\n"
"\t"

