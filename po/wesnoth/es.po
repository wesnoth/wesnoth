"\t\tprefix=$river{!}|$base{!}\n"
"\t\triver=$river{!}|del Río\n"
"\t\tsuffix= del Lecho| del Vado| del Cruce| {river} \n"
"\t\tmain={prefix}{suffix}\n"
"\t"

