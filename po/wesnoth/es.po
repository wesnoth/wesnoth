"\n"
"\t\tprefix=$base{!}|$river{!}\n"
"\t\triver=$river{!}|River\n"
"\t\tsuffix=ham|ford|cross| on {river} \n"
"\t\tmain={prefix}{suffix}\n"
"\t"
