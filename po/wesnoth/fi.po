msgid "female^poisoned"
msgstr "myrkytetty"

#: data/lua/wml-tags.lua:826 src/actions.cpp:1502
msgid "female^slowed"
msgstr "hidastettu"

#: data/lua/wml-tags.lua:826 src/actions.cpp:1502
msgid "slowed"
msgstr "hidastettu"

