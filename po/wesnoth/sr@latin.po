msgid "User Map"
msgstr "Korisnička mapa"

#: data/multiplayer.cfg:17 src/leader_list.cpp:104
msgid "Random"
msgstr "nasumično"

#: data/multiplayer.cfg:24
msgid "Default"
msgstr "osnovna"

#: data/multiplayer.cfg:37
msgid "Age of Heroes"
msgstr "Doba heroja"

#: data/multiplayer.cfg:50
msgid "Great War"
msgstr "Veliki rat"

