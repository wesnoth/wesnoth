msgid "Random"
msgstr "Náhodný"

#: data/multiplayer.cfg:24
msgid "Default"
msgstr "Standardní"

#: data/multiplayer.cfg:37
msgid "Age of Heroes"
msgstr "Věk hrdinů"

#: data/multiplayer.cfg:50
msgid "Great War"
msgstr "Velká válka"

