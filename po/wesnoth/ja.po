#. [advanced_preference]: type=combo
#: data/advanced_preferences.cfg:4
msgid "Compress saved games"
msgstr "セーブデータの圧縮"

#. [option]: id=gzip
#: data/advanced_preferences.cfg:9
msgid "save_compression^Gzip"
msgstr "Gzip"

#. [option]: id=gzip
#: data/advanced_preferences.cfg:10
