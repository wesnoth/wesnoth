msgid "Be more aggressive"
msgstr "更注重進攻"

#. [command]
#: data/core/macros/ai_controller.cfg:160
msgid "aggressive."
msgstr "注重進攻。"

#. [option]: type=behaviour
#: data/core/macros/ai_controller.cfg:177
msgid "Be more defensive"
msgstr "更注重防守"

#. [command]
#: data/core/macros/ai_controller.cfg:190
msgid "defensive."
msgstr "防禦。"

