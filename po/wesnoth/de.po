msgid "Introduction"
msgstr "Einführung"

#: data/help.cfg:16
msgid "Gameplay"
msgstr "Spielregeln"

#: data/help.cfg:22 data/help.cfg:209 src/playturn.cpp:2231
msgid "Traits"
msgstr "Charakteristika"

#: data/help.cfg:28 src/playturn.cpp:1776
msgid "Units"
