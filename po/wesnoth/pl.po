msgid "Introduction"
msgstr "Wprowadzenie"

#: data/help.cfg:16
msgid "Gameplay"
msgstr "Gra"

#: data/help.cfg:22 data/help.cfg:209 src/playturn.cpp:2123
msgid "Traits"
msgstr "Cechy"

#: data/help.cfg:28 src/playturn.cpp:1688
msgid "Units"
msgstr "Jednostki"

