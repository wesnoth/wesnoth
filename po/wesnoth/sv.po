msgid "Introduction"
msgstr "Introduktion"

#: data/help.cfg:16
msgid "Gameplay"
msgstr "Spelteknik"

#: data/help.cfg:22 data/help.cfg:209 src/playturn.cpp:2102
msgid "Traits"
msgstr "Karaktärsdrag"

#: data/help.cfg:28 src/playturn.cpp:1667
msgid "Units"
msgstr "Trupper"

