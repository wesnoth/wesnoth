#. [topic]: id=using_addons
#: data/core/help.cfg:497
#, fuzzy
msgid ""
"\n"
"\n"
"<header>text='Creator Resources'</header>"
msgstr ""
"\n"
"\n"
"<header>text='Verjetnost zadetka'</header>"

#. [topic]: id=using_addons
#: data/core/help.cfg:499
msgid ""
"\n"
"\n"
