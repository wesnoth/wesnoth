#: data/campaigns/Son_Of_The_Black_Eye/scenarios/The_Siege_Of_Barag_Gor.cfg:26
#, fuzzy
msgid "Break the Siege of Barag Gor"
msgstr "Die Sege van Barag Gór"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/The_Siege_Of_Barag_Gor.cfg:68
msgid "Etheliel"
msgstr "Etheliel"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/The_Siege_Of_Barag_Gor.cfg:116
msgid "Rugh"
msgstr "Rugh"

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/The_Siege_Of_Barag_Gor.cfg:137
msgid "Hida'tul"
msgstr ""

#: data/campaigns/Son_Of_The_Black_Eye/scenarios/The_Siege_Of_Barag_Gor.cfg:173
