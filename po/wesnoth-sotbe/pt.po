# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/05_To_the_Harbor_of_Tirigaz.cfg, line: 4
msgid "To the Harbor of Tirigaz"
msgstr "Para o Porto de Tirigaz"
#
# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/05_To_the_Harbor_of_Tirigaz.cfg, line: 163
# File: data/campaigns/Son_Of_The_Black_Eye/scenarios/05_To_the_Harbor_of_Tirigaz.cfg, line: 163
