#: data/scenarios/Son_Of_The_Black_Eye/Black_Flag.cfg:69
msgid "Transport Galleon"
msgstr "輸送用ガレオン船"

#: data/scenarios/Son_Of_The_Black_Eye/Black_Flag.cfg:71
msgid "Transport"
msgstr "輸送"

#: data/scenarios/Son_Of_The_Black_Eye/Black_Flag.cfg:81
#: data/scenarios/Son_Of_The_Black_Eye/Clash_Of_Armies.cfg:86
#: data/scenarios/Son_Of_The_Black_Eye/Desert_Of_Death.cfg:57
