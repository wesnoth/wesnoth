#. [set_variable]
#: data/campaigns/Under_the_Burning_Suns/utils/kaleh-abilities.cfg:37
msgid "female^stunned"
msgstr "atordada"

#. [set_variable]
#: data/campaigns/Under_the_Burning_Suns/utils/kaleh-abilities.cfg:44
