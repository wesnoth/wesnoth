#
# File: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg, line: 472
# File: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg, line: 472
# File: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg, line: 472
#. [message]: speaker=Goblin Coward
msgid "Run away!"
msgstr "Fujam!"

#
# File: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg, line: 477
# File: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg, line: 477
# File: data/campaigns/Under_the_Burning_Suns/scenarios/04_Descending_into_Darkness.cfg, line: 477
#. [message]: speaker=Zhul
msgid "Goblins are so predictable."
msgstr "Gnomos são tão predicareis."

#
