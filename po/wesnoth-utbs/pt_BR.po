msgid "Troll Avenger"
msgstr "Vingador Troll"

#
# File: data/campaigns/Under_the_Burning_Suns/scenarios/08_Out_of_the_Frying_Pan.cfg, line: 658
# File: data/campaigns/Under_the_Burning_Suns/scenarios/08_Out_of_the_Frying_Pan.cfg, line: 658
