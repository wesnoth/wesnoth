#: data/campaigns/Under_the_Burning_Suns/scenarios/06b_In_the_Domain_of_Dwarves.cfg:159
msgid "Dwarf Chieftain"
msgstr "patulj-poglavica"

#. [objective]: condition=win
#: data/campaigns/Under_the_Burning_Suns/scenarios/06b_In_the_Domain_of_Dwarves.cfg:238
msgid "Kill Dwarf Chieftain"
msgstr "Ubiješ li poglavicu patuljaka"

#. [event]
