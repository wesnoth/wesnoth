msgid "Weakened Central Body"
msgstr "oslabljeno središnje telo"

#. [unit_type]: id=Crawling Horror, race=monster
#: data/campaigns/Under_the_Burning_Suns/units/monsters/Alien_Minion.cfg:4
msgid "Crawling Horror"
msgstr "puzavi užas"

