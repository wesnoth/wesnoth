#. [event]
#: data/campaigns/The_South_Guard/scenarios/07b_Pebbles_in_the_Flood.cfg:132
msgid "Orome"
msgstr "Ороме"

#. [event]
#: data/campaigns/The_South_Guard/scenarios/07b_Pebbles_in_the_Flood.cfg:133
msgid "Mitche"
