msgstr "Herlin"

#. [side]: type=Draug, id=Fervin
#: data/campaigns/Northern_Rebirth/scenarios/04_Clearing_the_Mines.cfg:124
msgid "Fervin"
msgstr "Fervin"

#. [side]: type=Draug, id=Hellian
#: data/campaigns/Northern_Rebirth/scenarios/04_Clearing_the_Mines.cfg:139
