#: data/campaigns/Winds_of_Fate/scenarios/11_Crosswind.cfg:41
msgid "Flight Gorlack"
msgstr "Gorlakova letka"

#. [side]
#: data/campaigns/Winds_of_Fate/scenarios/04_Journey.cfg:73
#: data/campaigns/Winds_of_Fate/scenarios/04_Journey.cfg:111
msgid "Merfolk"
msgstr "Mořský lid"

