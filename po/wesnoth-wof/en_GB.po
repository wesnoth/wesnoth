msgstr "Wesmere March Guard"

#. [leader]: id=Vonel, type=Elvish Marshal
#: data/campaigns/Winds_of_Fate/scenarios/07_Harvest.cfg:198
msgid "Vonel"
msgstr "Vonel"

#. [side]
#: data/campaigns/Winds_of_Fate/scenarios/07_Harvest.cfg:204
