#: data/campaigns/Winds_of_Fate/units/Caravel.cfg:56
msgid "ballista"
msgstr "baliste"

#. [attack]: type=impact
#: data/campaigns/Winds_of_Fate/units/Caravel.cfg:65
msgid "polybolos"
msgstr "multi-bolas"

