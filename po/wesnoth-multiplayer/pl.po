msgid "Turn 11: Northerners"
msgstr "Tura 11: Ludy północy"

#. [label]
#: data/multiplayer/scenarios/6p_Team_Survival.cfg:471
msgid "Turn 14: Drakes"
msgstr "Tura 14: Smoki"

#. [label]
#: data/multiplayer/scenarios/6p_Team_Survival.cfg:476
