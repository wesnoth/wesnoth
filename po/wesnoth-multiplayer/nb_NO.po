#~ msgid "ensnare"
#~ msgstr "snarefange"

#~ msgid "thorns"
#~ msgstr "klunger"

#~ msgid "female^Elvish Enchantress"
#~ msgstr "Hulder"

#~ msgid "entangle"
#~ msgstr "innfiltre"

#~ msgid "faerie fire"
#~ msgstr "stjernestøv"

