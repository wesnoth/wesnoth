#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/04a_The_Swamp_of_Esten.cfg:118
msgid "Cultists"
msgstr "科尔蒂茨"

#. [side]: type=Dark Sorcerer, id=Lollyra
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/04a_The_Swamp_of_Esten.cfg:70
msgid "Lollyra"
msgstr "洛莱拉"

