msgid "Trolls"
msgstr "Troles"

#. [side]: type=Troll Warrior, id=Raol
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/17c_Troll_Hole.cfg:59
msgid "Raol"
msgstr "Raol"

#. [side]: type=Troll Warrior, id=Rilg
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/17c_Troll_Hole.cfg:80
