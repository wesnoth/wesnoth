msgstr "Cau de trols"

#. [side]: type=Troll Warrior, id=Erart
#: data/campaigns/The_Rise_Of_Wesnoth/scenarios/17c_Troll_Hole.cfg:38
msgid "Erart"
msgstr "Erart"

