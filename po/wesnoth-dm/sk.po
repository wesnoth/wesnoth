msgid "Leollyn"
msgstr "Leolyn"

#. [side]: type=Orcish Slayer, id=Hagha-Tan
#: data/campaigns/Delfadors_Memoirs/scenarios/04_Leollyn.cfg:56
msgid "Hagha-Tan"
msgstr "Hagha-Tan"

#. [side]
