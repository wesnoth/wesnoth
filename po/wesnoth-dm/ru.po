#: data/campaigns/Delfadors_Memoirs/terrain.cfg:12
msgid "Ethereal Abyss"
msgstr "Бездонная пропасть"

#. [terrain_type]: id=milestone1
#: data/campaigns/Delfadors_Memoirs/terrain.cfg:25
msgid "Milestone1"
msgstr "Указатель 1"

#. [terrain_type]: id=milestone2
