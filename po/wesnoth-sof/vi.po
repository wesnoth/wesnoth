#. [side]: type=Goblin Rouser, id=Pidmer
#: data/campaigns/Sceptre_of_Fire/scenarios/2p5_Reaching_the_Runecrafter.cfg:50
msgid "Pidmer Gar"
msgstr ""

#. [side]: type=Goblin Rouser, id=Pidmer
