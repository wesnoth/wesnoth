#. [side]
#: data/campaigns/The_Hammer_of_Thursagan/scenarios/13_Epilogue.cfg:53
msgid "Althasol"
msgstr "Altasol"

#. [side]
#: data/campaigns/The_Hammer_of_Thursagan/scenarios/13_Epilogue.cfg:54
msgid "Dulalas"
