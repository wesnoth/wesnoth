msgid "Masked Dwarves"
msgstr "覆面ドワーフ"

#. [side]
#: data/campaigns/The_Hammer_of_Thursagan/scenarios/05_Fear.cfg:78
msgid "Peasants"
msgstr ""

#. [side]
#: data/campaigns/The_Hammer_of_Thursagan/scenarios/05_Fear.cfg:106
