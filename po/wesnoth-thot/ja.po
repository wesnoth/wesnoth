#~ msgstr "Perrin 学長"

#~ msgid "Preceptor"
#~ msgstr "教師"

#~ msgid "Warder"
#~ msgstr "警備員"

#~ msgid "Drakes"
#~ msgstr "ドレーク"

#~ msgid "Glashal"
#~ msgstr "Glashal"

#~ msgid "Defeat Glashal"
#~ msgstr "Glashal を倒す"

#~ msgid ""
